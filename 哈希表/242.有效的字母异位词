class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s)!=len(t):
            return False
        count={}
        for i in s:
            count[i]=count.get(i,0)+1
        for j in t:
            if j not in count or count[j] == 0:
                return False
            count[j]-=1
            if count[j]==0:
                del count[j]
        return len(count)==0
